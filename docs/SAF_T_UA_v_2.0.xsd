<?xml version="1.0"?>

<xs:schema 
	id="SAF-T_UA"
	version="2.00" 
	elementFormDefault="qualified" 
	attributeFormDefault="unqualified" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema"> 
	
    <xs:element name="AuditFile">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Header">
					<xs:annotation>
						<xs:documentation>I. Заголовок</xs:documentation>
					</xs:annotation>		
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="HeaderStructure">
								<xs:sequence>
									<xs:element name="FundInfo" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Інформація про фонд</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="FundType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>Тип фонду</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="SAFcodeType">
															<xs:enumeration value="ПІФ"/>
															<xs:enumeration value="КІФ"/>
															<xs:enumeration value="НПФ"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="IJINumber" type="SAFshorttextType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>ЄДРІСІ</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="AMCRegistrationNumber" type="SAFshorttextType" minOccurs="0">
													<xs:annotation>
														<xs:documentation>ЄДРПОУ КУА/Адміністратора НПФ</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>	
									</xs:element>																			
									<xs:element name="BranchInfo" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Інформація про філію</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="BranchNumber" type="SAFlongtextType">
													<xs:annotation>
														<xs:documentation>Номер філії</xs:documentation>
													</xs:annotation>													
												</xs:element>
												<xs:element name="BranchName" type="SAFlongtextType">
													<xs:annotation>
														<xs:documentation>Назва філії</xs:documentation>
													</xs:annotation>													
												</xs:element>
											</xs:sequence>
										</xs:complexType>	
									</xs:element>																		
									<xs:element name="TaxAccountingBasis" type="SAFshorttextType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Метод податкового обліку</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="TaxEntity" type="SAFmiddle2textType" minOccurs="0" maxOccurs="unbounded">
										<xs:annotation>
											<xs:documentation>Відокремлений підрозділ</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="NumberOfParts" type="xs:positiveInteger" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Кількість частин файлу</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="PartNumber" type="xs:positiveInteger" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Порядковий номер частини файлу</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>	
                <xs:element name="MasterFiles" minOccurs="0">
					<xs:annotation>
						<xs:documentation>II. Довідники</xs:documentation>
					</xs:annotation>		
					<xs:complexType> 
						<xs:sequence>
							<xs:element name="AccountingPolicies" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Облікова політика</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AccountingPolicy" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Облікова політика</xs:documentation>
											</xs:annotation>
											<xs:complexType>  
												<xs:sequence>
													<xs:element name="NameOfAccountingPolicy" type="SAFstringType">
														<xs:annotation>
															<xs:documentation>Назва елемента облікової політики</xs:documentation>
														</xs:annotation>		
													</xs:element>
													<xs:element name="DescriptionOfAccountingPolicy" type="SAFstringType">
														<xs:annotation>
															<xs:documentation>Опис елемента облікової політики</xs:documentation>
														</xs:annotation>	
													</xs:element>
													<xs:element name="AccountingPolicyDocument" type="SAFstringType">
														<xs:annotation>
															<xs:documentation>Реквізити документа(ів) про облікову політику</xs:documentation>
														</xs:annotation>		
													</xs:element>
													<xs:element name="AccountingStandart" type="SAFstringType">
														<xs:annotation>
															<xs:documentation>Найменування ПСБО/МСБО, МСФЗ</xs:documentation>
														</xs:annotation>	
													</xs:element>
												</xs:sequence>	
											</xs:complexType>
										</xs:element>
									</xs:sequence>	
								</xs:complexType>									
							</xs:element>
							<xs:element name="TransactionFeatures" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Довідники операцій</xs:documentation>
								</xs:annotation>	
								<xs:complexType>
									<xs:sequence>
										<xs:element name="TransactionFeature" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Опис типу операції</xs:documentation>
											</xs:annotation>
											<xs:complexType>  
												<xs:sequence>
													<xs:element ref="TransactionType">
														<xs:annotation>
															<xs:documentation>Тип операції</xs:documentation>
														</xs:annotation>														
													</xs:element>
													<xs:element name="TransactionTypeDescription" type="SAFlongtextType">
														<xs:annotation>
															<xs:documentation>Опис операції</xs:documentation>
														</xs:annotation>															
													</xs:element>
												</xs:sequence>	
											</xs:complexType>
										</xs:element>
									</xs:sequence>	
								</xs:complexType>									
							</xs:element>
							<xs:element name="GeneralLedgerAccounts" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Сальдові/оборотні відомості</xs:documentation>
								</xs:annotation>				
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Account" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Рахунок бухгалтерського обліку</xs:documentation>
											</xs:annotation>
											<xs:complexType>  
												<xs:sequence>
													<xs:element ref="AccountID">
														<xs:annotation>
															<xs:documentation>Код рахунку</xs:documentation>
														</xs:annotation>		
													</xs:element>
													<xs:element name="AccountDescription" type="SAFlongtextType">
														<xs:annotation>
															<xs:documentation>Назва рахунку</xs:documentation>
														</xs:annotation>		
													</xs:element>
													<xs:element name="StandardAccountID" type="SAFmiddle1textType">
														<xs:annotation>
															<xs:documentation>Номер стандартного рахунку</xs:documentation>
														</xs:annotation>		
													</xs:element> 
													<xs:element name="GroupingCategory" type="SAFmiddle1textType">
														<xs:annotation>
															<xs:documentation>Категорія рахунку</xs:documentation>
														</xs:annotation>		
													</xs:element>
													<xs:element name="GroupingCode" type="SAFmiddle1textType">
														<xs:annotation>
															<xs:documentation>Код групи рахунку</xs:documentation>
														</xs:annotation>		
													</xs:element>
													<xs:element name="AccountType" type="SAFshorttextType" minOccurs="0">
														<xs:annotation>
															<xs:documentation>Тип рахунку</xs:documentation>
														</xs:annotation>												
													</xs:element>
													<xs:element name="AccountCreationDate" type="SAFdateType" minOccurs="0"> 
														<xs:annotation>
															<xs:documentation>Дата створення</xs:documentation>
														</xs:annotation>		
													</xs:element>
													<xs:choice maxOccurs="2">
														<xs:element ref="OpeningDebitBalance">
															<xs:annotation>
																<xs:documentation>Початкове дебетове сальдо</xs:documentation>
															</xs:annotation>		
														</xs:element>
														<xs:element ref="OpeningCreditBalance">
															<xs:annotation>
																<xs:documentation>Початкове кредитове сальдо</xs:documentation>
															</xs:annotation>		
														</xs:element>
													</xs:choice>		
													<xs:element ref="DebitTurnover">
														<xs:annotation>
															<xs:documentation>Дебетовий оборот</xs:documentation>
														</xs:annotation>		
													</xs:element>
													<xs:element ref="CreditTurnover">
														<xs:annotation>
															<xs:documentation>Кредитовий оборот</xs:documentation>
														</xs:annotation>		
													</xs:element>														
													<xs:choice maxOccurs="2">
														<xs:element ref="ClosingDebitBalance">
															<xs:annotation>
																<xs:documentation>Кінцеве дебетове сальдо</xs:documentation>
															</xs:annotation>		
														</xs:element> 
														<xs:element ref="ClosingCreditBalance">
															<xs:annotation>
																<xs:documentation>Кінцеве кредитове сальдо</xs:documentation>
															</xs:annotation>		
														</xs:element>
													</xs:choice>
												</xs:sequence>	
											</xs:complexType>
										</xs:element>
									</xs:sequence>	
								</xs:complexType>
							</xs:element>								
							<xs:element name="Taxonomies" minOccurs="0">	
								<xs:annotation>
									<xs:documentation>Таксономії</xs:documentation>
								</xs:annotation>	
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Taxonomy" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Таксономія</xs:documentation>
											</xs:annotation>
											<xs:complexType>  
												<xs:sequence>
													<xs:element name="TaxonomyReference" type="SAFlongtextType">
														<xs:annotation>
															<xs:documentation>Посилання на таксономію</xs:documentation>
														</xs:annotation>
													</xs:element>														
													<xs:element name="TaxonomyElement" minOccurs="0" maxOccurs="unbounded"> 
														<xs:annotation>
															<xs:documentation>Елемент таксономії</xs:documentation>
														</xs:annotation>
														<xs:complexType> 
															<xs:sequence>																
																<xs:element name="TaxonomyCode" type="SAFlongtextType">
																	<xs:annotation>
																		<xs:documentation>Код таксономії</xs:documentation>
																	</xs:annotation>
																</xs:element>	
																<xs:element name="TaxonomyClusterID" type="SAFlongtextType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Ідентифікатор кластера таксономії</xs:documentation>
																	</xs:annotation>
																</xs:element>	
																<xs:element name="TaxonomyClusterContextID" type="SAFlongtextType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Контекстний ідентифікатор кластера таксономії</xs:documentation>
																	</xs:annotation>
																</xs:element>	
																<xs:element ref="AccountID">
																	<xs:annotation>
																		<xs:documentation>Код рахунку</xs:documentation>
																	</xs:annotation>
																</xs:element>		
															</xs:sequence>
														</xs:complexType> 
													</xs:element>
												</xs:sequence>
											</xs:complexType> 
										</xs:element>
									</xs:sequence>
								</xs:complexType>	
							</xs:element>							
							<xs:element name="Customers" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Клієнти</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Customer" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Клієнт</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="CompanyStructure">														
														<xs:sequence>
															<xs:element ref="CustomerID">
																<xs:annotation>
																	<xs:documentation>Ідентифікатор покупця</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="CustomerType" minOccurs="0" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Тип особи покупця</xs:documentation>
																</xs:annotation>																
															</xs:element>
															<xs:element ref="RelatedPartyCode" minOccurs="0" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Код ознаки пов'язаності</xs:documentation>
																</xs:annotation>
															</xs:element>															
															<xs:element name="CustomerBalance" type="CounterAgentBalanceStructure" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Сальдо розрахунків</xs:documentation>
																</xs:annotation>
															</xs:element>	
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>								
							</xs:element>
							<xs:element name="Suppliers" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Постачальники</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Supplier" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Постачальник</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="CompanyStructure">
														<xs:sequence>
															<xs:element ref="SupplierID">
																<xs:annotation>
																	<xs:documentation>Ідентифікатор постачальника</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="SupplierType" minOccurs="0" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Тип особи постачальника</xs:documentation>
																</xs:annotation>																
															</xs:element>
															<xs:element ref="RelatedPartyCode" minOccurs="0" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Код ознаки пов'язаності</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="SupplierBalance" type="CounterAgentBalanceStructure" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>Сальдо розрахунків</xs:documentation>
																</xs:annotation>
															</xs:element>		
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>								
							</xs:element>	
							<xs:element name="TaxTable" minOccurs="0">	
								<xs:annotation>
									<xs:documentation>Таблиця податків</xs:documentation>
								</xs:annotation>	
								<xs:complexType>
									<xs:sequence>
										<xs:element name="TaxTableEntry" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Опис податку</xs:documentation>
											</xs:annotation>
											<xs:complexType>  
												<xs:sequence>
													<xs:element ref="TaxType">
														<xs:annotation>
															<xs:documentation>Вид податку</xs:documentation>
														</xs:annotation>
													</xs:element>	
													<xs:element ref="Description">
														<xs:annotation>
															<xs:documentation>Опис виду податку</xs:documentation>
														</xs:annotation>														
													</xs:element>	
													<xs:element name="TaxCodeDetails" maxOccurs="unbounded"> 
														<xs:annotation>
															<xs:documentation>Дані про податок</xs:documentation>
														</xs:annotation>		
														<xs:complexType> 
															<xs:sequence>
																<xs:element ref="TaxCode">
																	<xs:annotation>
																		<xs:documentation>Код податку</xs:documentation>
																	</xs:annotation>
																</xs:element>	
																<xs:element name="EffectiveDate" type="SAFdateType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Дата набрання чинності</xs:documentation>
																	</xs:annotation>
																</xs:element>	
																<xs:element name="ExpirationDate" type="SAFdateType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Дата закінчення</xs:documentation>
																	</xs:annotation>
																</xs:element>	
																<xs:element ref="Description">
																	<xs:annotation>
																		<xs:documentation>Опис коду податку</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:choice minOccurs="0">
																	<xs:element ref="TaxPercentage">
																		<xs:annotation>
																			<xs:documentation>Відносна ставка податку</xs:documentation>
																		</xs:annotation>
																	</xs:element>	
																	<xs:element name="FlatTaxRate" type="AmountStructure">
																		<xs:annotation>
																			<xs:documentation>Абсолютна ставка податку</xs:documentation>
																		</xs:annotation>
																	</xs:element>	
																</xs:choice>		
																<xs:element ref="Country" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Країна</xs:documentation>
																	</xs:annotation>
																</xs:element>	
																<xs:element name="Region" type="SAFmiddle1textType" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Регіон</xs:documentation>
																	</xs:annotation>
																</xs:element>				
															</xs:sequence>
														</xs:complexType> 
													</xs:element>
												</xs:sequence>
											</xs:complexType> 
										</xs:element>
									</xs:sequence>
								</xs:complexType>	
							</xs:element>
							<xs:element name="UOMTable" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Таблиця одиниць виміру</xs:documentation>
								</xs:annotation>		
								<xs:complexType>
									<xs:sequence>
										<xs:element name="UOMTableEntry" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Дані про одиницю виміру</xs:documentation>
											</xs:annotation>
											<xs:complexType>  
												<xs:sequence>
													<xs:element ref="UnitOfMeasure">
														<xs:annotation>
															<xs:documentation>Одиниця виміру</xs:documentation>
														</xs:annotation>
													</xs:element>	
													<xs:element ref="Description">
														<xs:annotation>
															<xs:documentation>Опис одиниці виміру</xs:documentation>
														</xs:annotation>
													</xs:element>	
												</xs:sequence>
											</xs:complexType> 
										</xs:element>
									</xs:sequence>
								</xs:complexType>	
							</xs:element>		
							<xs:element name="AnalysisTypeTable" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Таблиця аналітичних рахунків</xs:documentation>
								</xs:annotation>	
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AnalysisTypeTableEntry" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Аналітичний рахунок</xs:documentation>
											</xs:annotation>
											<xs:complexType>  
												<xs:sequence>
													<xs:element ref="AnalysisType">
														<xs:annotation>
															<xs:documentation>Код типу аналітичного рахунку (субконто)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="AnalysisTypeDescription" type="SAFlongtextType">
														<xs:annotation>
															<xs:documentation>Назва типу аналітичного рахунку (субконто)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element ref="AnalysisID">
														<xs:annotation>
															<xs:documentation>Код аналітичного рахунку (субконто)</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="AnalysisIDDescription" type="SAFlongtextType">
														<xs:annotation>
															<xs:documentation>Назва аналітичного рахунку (субконо)</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType> 
										</xs:element>
									</xs:sequence>
								</xs:complexType>	
							</xs:element>								
							<xs:element name="MovementTypeTable" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Таблиця типів руху</xs:documentation>
								</xs:annotation>	
								<xs:complexType>
									<xs:sequence>
										<xs:element name="MovementTypeTableEntry" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Дані типу руху запасів</xs:documentation>
											</xs:annotation>
											<xs:complexType>  
												<xs:sequence>
													<xs:element ref="MovementType">
														<xs:annotation>
															<xs:documentation>Тип руху</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element ref="Description">
														<xs:annotation>
															<xs:documentation>Опис типу руху</xs:documentation>
